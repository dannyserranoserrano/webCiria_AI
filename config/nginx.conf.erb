worker_processes 1;

events { worker_connections 1024; }

http {
  include       mime.types;
  default_type  text/html;

  sendfile        on;
  keepalive_timeout  65;

  server {
    listen <%= ENV["PORT"] %>;
    root /public;

    location / {
      index index.html;
      try_files $uri /index.html;
    }
  }
}
